<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import fxml2.*?>

<VBox spacing="5"  xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<padding><Insets bottom="5" top="5" left="5" right="5"/></padding>
	
	<children>
	
		<Label text="$item" >
			<Item fx:id="item" id="111" name="Item 111"/>
		</Label>
		
		<Label text="$copiedItem" >
			<fx:copy fx:id="copiedItem" source="item" />
		</Label>
		
	</children>
	
</VBox>
